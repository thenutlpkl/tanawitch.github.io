{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "noEmit": true,
    "declaration": true,
    "outDir": "./dist"
  },
  "include": ["vite.config.ts"]
}